function Calcular() {
    let prod = document.querySelector("#prod").value;
    let vr = document.querySelector("#vr").value;
    let qtd = document.querySelector("#qtd").value;
    let pagamento = document.querySelector("#pagamento").value;
  
    let total = qtd * vr;

   
    document.querySelector("#vlprod").innerHTML=`O valor total Ã©: ${total} o produto desejado foi: ${prod} O formato de pagamento foi: ${pagamento}`

  }
  

